@startuml
class Usuario {
  +id: int
  +nome: string
  +email: string
}

class Passageiro {
  +cpf: string
}

class Administrador {
  +cargo: string
}

class Empresa {
  +id: int
  +nome: string
  +cnpj: string
}

class TicketReporte {
  +id: int
  +descricao: string
  +data: Date
  +status: string
}

class PontoDeOnibus {
  +id: int
  +localizacao: string
}

class EstacaoDeMetro {
  +id: int
  +nome: string
  +linha: string
}

class ProblemaDeAcessibilidade {
  +id: int
  +descricao: string
  +gravidade: string
}

class TransicaoStatus {
   +id: int
   +String: statusInicial
   +String: statusAtualizado
   +TicketReporte: ticketAtualizado
   +Admnistrador: admAtualizacao
}

Usuario <|-- Passageiro
Usuario <|-- Administrador
